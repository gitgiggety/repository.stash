<?xml version='1.0' encoding='UTF-8'?>
<addons>
<addon id="plugin.video.stash" name="Stash Kodi Plugin" version="0.0.1" provider-name="gitgiggety">
    <requires>
        <import addon="xbmc.python" version="3.0.0" />
        <import addon="script.module.requests" version="2.9.1" />
    </requires>
    <extension point="xbmc.python.pluginsource" library="addon.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary lang="en_GB">View videos tracked in Stash</summary>
        <description lang="en_GB">Plugin to browse and view videos which are tracked in Stash (stashapp.io)</description>
        <language />
        <platform>all</platform>
        <license>GNU GENERAL PUBLIC LICENSE Version 2, June 1991</license>
        <forum />
        <website />
        <email>gitgiggety@outlook.com</email>
        <source />
        <news />
        <disclaimer />
        <assets>
            <icon>resources/stash_logo.png</icon>
            <fanart />
        </assets>
    </extension>
</addon><addon id="repository.stash" name="repository.stash" version="1.0" provider-name="gitgiggety">
    <extension point="xbmc.addon.repository" name="Stash Repo">
        <dir>
            <info compressed="false">https://raw.githubusercontent.com/gitgiggety/repository.stash/master/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/gitgiggety/repository.stash/master/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/gitgiggety/repository.stash/master/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary lang="en_GB">Stash Repo</summary>
        <description lang="en_GB">Repository for Stash addon</description>
        <disclaimer />
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
    </extension>
</addon></addons>